<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_checked="true"
        android:state_enabled="false"
        android:drawable="@drawable/btn_checkbox_selected_disabled" />
    <item
        android:state_checked="false"
        android:state_enabled="false"
        android:drawable="@drawable/btn_checkbox_disabled" />
    <item
        android:state_checked="true"
        android:state_focused="true"
        android:drawable="@drawable/btn_checkbox_selected_focused" />
    <item
        android:state_checked="false"
        android:state_focused="true"
        android:drawable="@drawable/btn_checkbox_focused" />
    <item
        android:state_checked="true"
        android:drawable="@drawable/btn_checkbox_selected" />
    <item
        android:drawable="@drawable/btn_checkbox" />
</selector>
